            android:textColor="@color/white"
            android:textColorHint="@color/LightGrey" />


        <Button
            android:id="@+id/parallelogram_side_calc_button"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/parallelogram_side_perimeter_et"
            android:layout_marginTop="5dp"
            android:background="@drawable/black_button"
            android:gravity="center"
            android:text="@string/calculate"
            android:textColor="@color/white"
            android:textSize="13sp" />


        <TextView
            android:id="@+id/parallelogram_side_calc_answer_tx"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@+id/parallelogram_side_calc_button"
            android:layout_marginTop="5dp"
            android:background="@drawable/black_button"
            android:gravity="center"
            android:maxLines="1"
            android:textColor="@color/white"
            android:textSize="21sp" />

        <Button
            android:id="@+id/parallelogram_side_clear_button"
            android:layout_width="match_parent"
            android:layout_height="42dp"
            android:layout_below="@id/parallelogram_side_calc_answer_tx"
            android:layout_marginTop="5dp"
            android:background="@drawable/black_button"
            android:text="@string/clear"
            android:textColor="@color/white"
            android:textSize="13sp" />



        <View
            android:id="@+id/hline_5"
            android:layout_width="fill_parent"
            android:layout_height="5dp"
            android:background="@drawable/list_divider"
            android:layout_below="@id/parallelogram_side_clear_button"
            android:layout_marginTop="35dp"/>


    </RelativeLayout>

